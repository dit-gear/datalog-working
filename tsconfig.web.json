{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/**/*.d.ts",
    "src/shared/**/*.ts",
    "src/main/core/app-state/state.tsx"
, "src/shared/utils/getAttachments.ts", "src/shared/utils/getLatestDatalog.ts"],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/renderer/src/components/*"
      ],
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@shared/*" : [
        "src/shared/*"
      ],
      "@main/*": [
        "src/main/*"
      ]
    }
  }
}
